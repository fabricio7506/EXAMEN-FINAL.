<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Define el color de fondo del botón -->
    <solid android:color="#4BCB77" />

    <!-- Define el radio de los bordes redondeados -->
    <corners android:radius="20dp" />

    <!-- Define el padding interno del botón -->
    <padding
        android:left="16dp"
        android:top="8dp"
        android:right="16dp"
        android:bottom="8dp" />
</shape>
